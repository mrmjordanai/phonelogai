{
  "expo": {
    "name": "PhoneLog AI",
    "slug": "phonelogai",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.phonelogai.app"
    },
    "android": {
      "package": "com.phonelogai.app",
      "permissions": [
        "READ_PHONE_STATE",
        "READ_CALL_LOG",
        "READ_SMS",
        "READ_CONTACTS"
      ]
    },
    "web": {},
    "plugins": [
      "expo-contacts"
    ]
  }
}